 datatop/Android.mk  | 3 ++-
 rmnetctl/Android.mk | 3 ++-
 2 files changed, 4 insertions(+), 2 deletions(-)

diff --git a/datatop/Android.mk b/datatop/Android.mk
index 8338432..e1c3023 100644
--- a/datatop/Android.mk
+++ b/datatop/Android.mk
@@ -1,2 +1,3 @@
