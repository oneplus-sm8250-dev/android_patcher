 include $(call all-subdir-makefiles)
-
+endif
diff --git a/rmnetctl/Android.mk b/rmnetctl/Android.mk
index 8338432..e1c3023 100644
--- a/rmnetctl/Android.mk
+++ b/rmnetctl/Android.mk
@@ -1,2 +1,3 @@
